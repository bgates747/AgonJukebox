04223C 21 47 22 04 0121*        ld hl,@cmd
042240 01 05 00 00 0122*        ld bc,@end-@cmd
042244 5B DF       0123*        rst.lil $18
042246 C9          0124*        ret
042247 17 00 85    0125*    @cmd:       db 23, 0, 0x85
