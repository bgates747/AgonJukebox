04185A 32 85 18 04 0354*       ld (@bufferId0+2),a
04185E 3E 17       0355*       ld a,23
041860 32 9B 18 04 0356*       ld (@bufferId1+2),a
041864 3E 18       0357*       ld a,24
041866 32 92 18 04 0358*       ld (@bufferId2+2),a
04186A AF          0359*       xor a
