043500             0001*    ; AGON Movie (.agm) Header Structure Offsets and Descriptions
043500             0002*    ; relative to the end of the .agm file's .wav header
043500             0003*    agn_header_start:  EQU 76   ; wav_header_size
043500             0004*    agm_magic:         EQU 0    ; 6 bytes: "AGNMOV" identifier
043500             0005*    agm_version:       EQU 6    ; 1 byte: File format version (e.g., 0x01)
043500             0006*    agm_width:         EQU 7    ; 1 byte: Video width in pixels (e.g., 128)
043500             0007*    agm_height:        EQU 8    ; 1 byte: Video height in pixels (e.g., 96)
043500             0008*    agm_frame_rate:    EQU 9    ; 1 byte: Frames per second (e.g., 4)
043500             0009*    agm_total_frames:  EQU 10   ; 4 bytes: Total number of frames (32-bit integer)
043500             0010*    agm_audio_seconds: EQU 14   ; 4 bytes: Total seconds of audio (32-bit float or int)
043500             0011*    agm_reserved:      EQU 18   ; 52 bytes: Reserved for future features (compression, metadata, etc.)
043500             0012*    agm_header_size:   EQU 68   ; Total AGN Header Size: 68 bytes
