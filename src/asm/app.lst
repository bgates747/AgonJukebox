045542 DD 17 00    0056*        ld de,(ix)               ; DE = address of the next string to compare
045545 CD 24 55 04 0057*        call alpha_asc           ; Compare (HL) and (DE)
045549 30 06       0058*        jr nc,@skip_swap         ; If (HL) <= (DE), skip swapping the strings
