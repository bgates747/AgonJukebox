04145E 17 1B 21    0479*       db 23,27,0x21 ; create bitmap from buffer
041461 00 00       0480*   @width: dw 0x0000
041463 00 00       0481*   @height: dw 0x0000
041465 00          0482*       db 0x00 ; rgba8888 format
