041D38 3E 17       0440*        ld a,23
041D3A 32 75 1D 04 0441*        ld (@bufferId1+2),a
041D3E 3E 18       0442*        ld a,24
041D40 32 6C 1D 04 0443*        ld (@bufferId2+2),a
041D44 AF          0444*        xor a
041D45 32 7C 1D 04 0445*        ld (@height+2),a
